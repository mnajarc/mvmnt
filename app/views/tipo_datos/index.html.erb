<p id="notice"><%= notice %></p>

<h1>Listing Tipo Datos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo dato</th>
      <th>Dataprot</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_datos.each do |tipo_dato| %>
      <tr>
        <td><%= tipo_dato.tipo_dato %></td>
        <td><%= tipo_dato.dataprot %></td>
        <td><%= link_to 'Show', tipo_dato %></td>
        <td><%= link_to 'Edit', edit_tipo_dato_path(tipo_dato) %></td>
        <td><%= link_to 'Destroy', tipo_dato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo dato', new_tipo_dato_path %>
