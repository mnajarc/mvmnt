<%- model_class = TipoAlmacen -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:clave_tipo_almacen) %></th>
      <th><%= model_class.human_attribute_name(:nombre_tipo_almacen) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @tipo_almacens.each do |tipo_almacen| %>
      <tr>
        <td><%= link_to tipo_almacen.clave_tipo_almacen, tipo_almacen_path(tipo_almacen) %></td>
        <td><%= tipo_almacen.nombre_tipo_almacen %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_tipo_almacen_path(tipo_almacen), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      tipo_almacen_path(tipo_almacen),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_tipo_almacen_path,
            :class => 'btn btn-primary' %>
