<p id="notice"><%= notice %></p>

<h1>Listado de Tipo Personas</h1>

<table>
  <thead>
    <tr>
      <th>Tipo persona</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_personas.each do |tipo_persona| %>
      <tr>
        <td><%= tipo_persona.tipo_persona %></td>
        <td><%= link_to 'Show', tipo_persona %></td>
        <td><%= link_to 'Edit', edit_tipo_persona_path(tipo_persona) %></td>
        <td><%= link_to 'Destroy', tipo_persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo persona', new_tipo_persona_path %>
