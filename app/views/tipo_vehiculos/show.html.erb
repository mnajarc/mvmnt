<%- model_class = TipoVehiculo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:clave_tipo_vehiculo) %>:</strong></dt>
  <dd><%= @tipo_vehiculo.clave_tipo_vehiculo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_vehiculo) %>:</strong></dt>
  <dd><%= @tipo_vehiculo.tipo_vehiculo %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              tipo_vehiculos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tipo_vehiculo_path(@tipo_vehiculo), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tipo_vehiculo_path(@tipo_vehiculo),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>