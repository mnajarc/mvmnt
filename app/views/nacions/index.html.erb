<p id="notice"><%= notice %></p>

<h1>Listado de Nacions</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nacions.each do |nacion| %>
      <tr>
        <td><%= nacion.nombre %></td>
        <td><%= link_to 'Show', nacion %></td>
        <td><%= link_to 'Edit', edit_nacion_path(nacion) %></td>
        <td><%= link_to 'Destroy', nacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nacion', new_nacion_path %>
