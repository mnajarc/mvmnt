<%- model_class = RutaTransporte -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>
:clave_ruta, :nombre_ruta, :kilometraje, :punto_a_id, :punto_b_id, :_destroy
<dl class="dl-horizontal">

    <dt><strong><%= model_class.human_attribute_name(:clave_ruta) %>:</strong></dt>
    <dd><%= @ruta_transporte.clave_ruta %></dd>
    <dt><strong><%= model_class.human_attribute_name(:nombre_ruta) %>:</strong></dt>
    <dd><%= @ruta_transporte.nombre_ruta %></dd>
    <dt><strong><%= model_class.human_attribute_name(:kilometraje) %>:</strong></dt>
    <dd><%= @ruta_transporte.kilometraje %></dd>
    <dd><%= @ruta_transporte.punto_a_id %></dd>
    <dd><%= @ruta_transporte.punto_b_id %></dd>

</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              ruta_transportes_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ruta_transporte_path(@ruta_transporte), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ruta_transporte_path(@ruta_transporte),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>

